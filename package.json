{
  "name": "kotomi-ui",
  "version": "1.19.3",
  "description": "基于Ant Design做的组件扩展，开发的一套组件库",
  "scripts": {
    "start": "dumi dev",
    "start:storybook": "start-storybook -p 3000",
    "build": "antd-tools run compile",
    "build:dumi": "dumi build docs/",
    "build:typedoc": "typedoc --out build/docs/typedoc",
    "build:storybook": "build-storybook  -c .storybook -o docs",
    "lint": "antd-tools run ts-lint",
    "lint-fix": "antd-tools run ts-lint-fix",
    "test": "jest"
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "repository": "https://github.com/Kotomi-Team/kotomi-ui",
  "homepage": "https://kotomi.site",
  "bugs": {
    "url": "https://github.com/Kotomi-Team/kotomi-ui/issues"
  },
  "keywords": [
    "React",
    "Ant Design",
    "Table"
  ],
  "author": "zhangj",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@ant-design/tools": "8.0.4",
    "@babel/core": "7.8.3",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-console": "1.2.1",
    "@storybook/addon-links": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/preset-ant-design": "0.0.1",
    "@storybook/preset-typescript": "1.1.0",
    "@storybook/react": "5.2.8",
    "@types/classnames": "2.2.10",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.149",
    "@types/rc-switch": "^1.9.0",
    "@types/react-color": "3.0.1",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.5",
    "@types/react-resizable": "1.7.2",
    "@types/shortid": "0.0.29",
    "@types/vfile-message": "2.0.0",
    "@typescript-eslint/eslint-plugin": "2.14.0",
    "@typescript-eslint/parser": "2.14.0",
    "babel-loader": "8.0.6",
    "babel-plugin-optimize-clsx": "2.6.1",
    "css-loader": "3.5.3",
    "dumi": "1.0.28",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.17.0",
    "husky": "4.0.0",
    "jest": "24.9.0",
    "less": "3.12.2",
    "less-loader": "5.0.0",
    "react-docgen-typescript-loader": "3.6.0",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.3",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "typedoc": "0.15.3",
    "typescript": "3.7.2",
    "webpack": "4.41.6"
  },
  "dependencies": {
    "@types/rc-time-picker": "^3.4.1",
    "ant-virtual-table": "0.1.10",
    "asp-antd-compatible": "3.26.20",
    "classnames": "2.2.6",
    "lodash": "4.17.21",
    "rc-query": "0.0.21",
    "react": "16.12.0",
    "react-color": "2.18.0",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dom": "16.12.0",
    "react-quill": "1.3.5",
    "react-resizable": "1.10.1",
    "shortid": "2.2.15",
    "xlsx": "0.17.0"
  },
  "resolutions": {}
}
